test #1
input: лена=имя дон=река москва=город False=ложь 3=удовлетворительно True=истина
output: ('True', 'истина') ('дон', 'река') ('лена', 'имя') ('москва', 'город')

test #2
input: house=дом horse=лошадь 3=число False=0 True=1
output: ('True', '1') ('horse', 'лошадь') ('house', 'дом')

test #3
input: лена=имя дон=река москва=город True=истина three=3 ложь=False
output: ('True', 'истина') ('three', '3') ('дон', 'река') ('лена', 'имя') ('ложь', 'False') ('москва', 'город')

test #4
input: лена=имя дон=река москва=город True=истина ложь=False
output: ('True', 'истина') ('дон', 'река') ('лена', 'имя') ('ложь', 'False') ('москва', 'город')

test #5
input: house=дом horse=лошадь 5=число False=0 True=1
output: ('5', 'число') ('True', '1') ('horse', 'лошадь') ('house', 'дом')
