test #1
input: 
+71234567890 Сергей
+71234567810 Сергей
+51234567890 Михаил
+72134567890 Николай
output: ('Михаил', ['+51234567890']) ('Николай', ['+72134567890']) ('Сергей', ['+71234567890', '+71234567810'])

test #2
input: 
+71234567890 Сергей
+71234567810 Сергей
+51234567890 Михаил
+51554567890 Михаил
+72134567890 Иван
+21234567890 Федор
output: ('Иван', ['+72134567890']) ('Михаил', ['+51234567890', '+51554567890']) ('Сергей', ['+71234567890', '+71234567810']) ('Федор', ['+21234567890'])
